#include<stdio.h>
int main()
{
	int n,r,as=0,temp;
	printf("Name :G Venkata Chalapathi\nReg no :192211150");
	printf("\n\nEnter any number :");
	scanf("%d",&n);
	temp=n;
	while(temp!=0)
	{
		r=temp%10;
		as=as+r*r*r;
		temp=temp/10;
	}
	if(as==n)
	{
		printf("Armstrong number..");
	}
	else
	{
		printf("Not an armstrong number..");
	}
}
