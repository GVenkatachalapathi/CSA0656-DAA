#include <stdio.h>
int main() 
{
	printf("Name :G Venkata Chalapathi\nReg no :192211150");
    int number, isPrime = 1;
    printf("\n\nEnter a number: ");
    scanf("%d", &number);
    if (number <= 1) {
        isPrime = 0;
    } else if (number == 2) {
        isPrime = 1; 
    } else if (number % 2 == 0) {
        isPrime = 0; 
    } else {
        for (int i = 3; i * i <= number; i += 2) {
            if (number % i == 0) {
                isPrime = 0;
                break;
            }
        }
    }

    if (isPrime) {
        printf("%d is a prime number.\n", number);
    } else {
        printf("%d is not a prime number.\n", number);
    }

    return 0;
}
