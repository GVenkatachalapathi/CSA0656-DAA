#include<stdio.h>
int main()
{
	int arr[100],i,n;
	printf("Name :G Venkata Chalapathi\nReg no :192211150");
	printf("\n\nEnter no of elements :");
	scanf("%d",&n);
	printf("Enter elements ");
	for(i=0;i<n;i++)
	{
		scanf("%d",&arr[i]);
	}
	int max = arr[0];
	for(i=1;i<n;i++)
	{
		if(arr[i]>max)
		{
			max = arr[i];
		}
	}
	printf("Largest number in array :%d",max);
}
